{
  "name": "learn-fastify-restful-api",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/myoxine/learn-fastify-restful-api.git",
  "author": "Hadi Halim <hdi_hlim@yahoo.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "node --env-file=.env.production ./dist/index.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "knex:init": "knex init -x ts",
    "knex:run": "knex --knexfile src/configs/knexfile.ts",
    "migrate:currentVersion": "npm run knex:run -- migrate:currentVersion",
    "migrate:make":"npm run knex:run -- migrate:make --",
    "migrate:list":"npm run knex:run -- migrate:list",
    "migrate:latest":"npm run knex:run -- migrate:latest --debug",
    "migrate:rollback": "npm run knex:run -- migrate:rollback --all --debug",
    "seed:make": "npm run knex:run -- seed:make --",
    "seed:run": "npm run knex:run -- seed:run --debug",
    "migrate:development:make": "cross-env NODE_ENV=development npm run migrate:make",
    "migrate:development:list": "cross-env NODE_ENV=development npm run migrate:list",
    "migrate:development:latest": "cross-env NODE_ENV=development npm run migrate:latest",
    "migrate:development:rollback": "cross-env NODE_ENV=development npm run migrate:rollback",
    "seed:development:make": "cross-env NODE_ENV=development npm run seed:make",
    "seed:development:run": "cross-env NODE_ENV=development npm run seed:run",
    "migrate:production:make": "cross-env NODE_ENV=development npm run migrate:make",
    "migrate:production:list": "cross-env NODE_ENV=development npm run migrate:list",
    "migrate:production:latest": "cross-env NODE_ENV=development npm run migrate:latest",
    "migrate:production:rollback": "cross-env NODE_ENV=development npm run migrate:rollback",
    "seed:production:make": "cross-env NODE_ENV=development npm run seed:make",
    "seed:production:run": "cross-env NODE_ENV=development npm run seed:run",
    "migrate:staging:make": "cross-env NODE_ENV=development npm run migrate:make",
    "migrate:staging:list": "cross-env NODE_ENV=development npm run migrate:list",
    "migrate:staging:latest": "cross-env NODE_ENV=development npm run migrate:latest",
    "migrate:staging:rollback": "cross-env NODE_ENV=development npm run migrate:rollback",
    "seed:staging:make": "cross-env NODE_ENV=development npm run seed:make",
    "seed:staging:run": "cross-env NODE_ENV=development npm run seed:run"
  },
  "devDependencies": {
    "@types/node": "^22.5.0",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.4",
    "pino-pretty": "^11.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^4.1.0",
    "ajv-errors": "^3.0.0",
    "env-schema": "^6.0.0",
    "fastify": "^4.28.1",
    "fastify-swagger": "^5.2.0",
    "json-schema-to-ts": "^3.1.1",
    "knex": "^3.1.0",
    "pg": "^8.12.0"
  }
}
